.check:not(:checked),
.check:checked {
  position: absolute;
  left: -9999px;
}
.check:not(:checked) + label,
.check:checked + label {  
  position: relative;
  overflow: hidden;
  padding-left: 35px;
  cursor: pointer;
  display: inline-block;
  height: 25px;
  line-height: 25px;

  -webkit-user-select: none; /* webkit (safari, chrome) browsers */
  -moz-user-select: none; /* mozilla browsers */
  -khtml-user-select: none; /* webkit (konqueror) browsers */
  -ms-user-select: none; /* IE10+ */
}

/* checkbox aspect */
.check + label:before,
.check + label:after {
  content: '';
  position: absolute;
  left: 0;
  z-index: 1;

  -webkit-transition: .2s;
  transition: .2s;
}
/* Unchecked styles */
.check:not(:checked) + label:before {
  top: 0px;
  width: 19px; height: 19px;
  border: 3px solid #85c555;
}
.check:not(:checked) + label:after {
  top: 0px;
  width: 19px; height: 19px;
  border: 3px solid #eee;
  z-index: 0;
}
/* Checked styles */
.check:checked + label:before {
  top: 2px;
  width: 6px; height: 12px;
  border-top: 3px solid transparent;
  border-left: 3px solid transparent;
  border-right: 3px solid #ffffff;
  border-bottom: 3px solid #ffffff;
  -webkit-transform: rotateZ(37deg);
  transform: rotateZ(37deg);

  -webkit-transform-origin: 20% 40%;
  transform-origin: 100% 100%;
}
.check:checked + label:after {
  top: 0px;
  width: 19px; height: 19px;
  border: 3px solid #1d6687;
  background-color: #1d6687;
  z-index: 0;
}
.check:disabled + label {
  color: #1d6687;
}
.check:disabled:not(:checked) + label:hover:before {
  border-color: #1d6687;
}

/* hover style just for information */
.check:not(:checked) + label:hover:before {
  border-color: #85c555;
}


