/*------------------------ efecto en los iconos del menu ------------------------*/

.dropdown li:hover span{
	
	background:url('../images/icons/25/icons-white.png') no-repeat left top!important;
}


.menu_bar {
	display:none;
}
 
@media screen and (max-width:1175px ) 
{
    
    #nav 
    {
        display: none;
		width:99%;	
		padding-left: 0px;	
	}
 
	#nav li
	{
    width:100%;
	background: #ffffff;
    background: -moz-linear-gradient(top, #ffffff, #fafafa);
    background: linear-gradient(to bottom, #ffffff, #fafafa);
    background: -webkit-gradient(linear, 0% 50%, 0% 60%, from(#ffffff), to(#fafafa));
    border:1px solid #fafafa;
    color:#16719a;	
    border:1px solid #1d6687;
    
	
    }
    
    #nav li a
    {
        color:#16719a;
    }
    #nav li:hover{
	   background: #e6e8eb;
	   color:#1d6687;
    }
	
	.menu_bar {
		display:block;
		width:100%;
	}
 
	.menu_bar .bt-menu {
		display:block;
		padding:3px;
		
		font-weight: bold;
		font-size:20px;
		-webkit-transition: left 0.3s  linear;
	    -webkit-transition-property:background;
	    -moz-transition: left 0.3s linear;
	    -moz-transition-property:background;
	    -o-transition: left 0.3s  linear;
	    -o-transition-property:background;
	    -ms-transition: left 0.3s linear;
	    -ms-transition-property:background;
	    transition: left 0.3s linear;
	    transition-property:background;
		
		background: #1d6687;
        background: -moz-linear-gradient(top, #1d6687, #165e7e);
        background: linear-gradient(to bottom, #1d6687, #165e7e);
        background: -webkit-gradient(linear, 0% 50%, 0% 60%, from(#1d6687), to(#165e7e));
        border:1px solid #165e7e;
        color:#f9f9f9;	
	}
	
	.menu_bar .bt-menu:hover{background: #247397;color:#ffffff;}
}