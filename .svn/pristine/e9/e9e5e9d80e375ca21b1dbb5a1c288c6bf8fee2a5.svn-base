/*------------------------------------- BACKGROUNDS ----------------------------------------------------*/
/*------------------------------------- BACKGROUNDS ----------------------------------------------------*/
/*------------------------------------- BACKGROUNDS ----------------------------------------------------*/
#nav li{
	
	background: #1b6079;
	background: -moz-linear-gradient(top, #1b6079 0%, #2282a5 50%, #1b6079 100%);
	background: -webkit-gradient(left top, left bottom, color-stop(0%, #1b6079), color-stop(50%, #2282a5), color-stop(100%, #1b6079));
	background: -webkit-linear-gradient(top, #1b6079 0%, #2282a5 50%, #1b6079 100%);
	background: -o-linear-gradient(top, #1b6079 0%, #2282a5 50%, #1b6079 100%);
	background: -ms-linear-gradient(top, #1b6079 0%, #2282a5 50%, #1b6079 100%);
	background: linear-gradient(to bottom, #1b6079 0%, #2282a5 50%, #1b6079 100%);
}
#nav li:hover{
	
	background: #cecece;
	background: -moz-linear-gradient(top, #cecece 0%, #f6f6f6 50%, #cecece 100%);
	background: -webkit-gradient(left top, left bottom, color-stop(0%, #cecece), color-stop(50%, #f6f6f6), color-stop(100%, #cecece));
	background: -webkit-linear-gradient(top, #cecece 0%, #f6f6f6 50%, #cecece 100%);
	background: -o-linear-gradient(top, #cecece 0%, #f6f6f6 50%, #cecece 100%);
	background: -ms-linear-gradient(top, #cecece 0%, #f6f6f6 50%, #cecece 100%);
	background: linear-gradient(to bottom, #cecece 0%, #f6f6f6 50%, #cecece 100%);
	
}
