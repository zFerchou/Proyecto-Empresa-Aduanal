#header-layer .nav-container{width:auto;max-width:100%;min-width:80%;margin:0px auto;}
#nav{
	display: inline;
	margin:0px;
	text-align:center;
	list-style:none;
	height:52px;
	width:auto;
    position:relative;
    line-height:20px;    
   	z-index:10;
   	position:absolute;

	
}
#nav li{
	
	float:left;
	width:185px;
	padding: 13px 0px 13px 0px;
	border-right:1px solid #c2c4c4;
	position:relative;
	
	-moz-box-shadow:1px 5px 10px #969696;
    -webkit-box-shadow: 1px 5px 10px #969696;
     box-shadow:1px 5px 10px #969696;	
}
/*------ Links -------*/
#nav li a{
	
	
	padding:18px 10px 18px 10px;
	text-decoration:none;
	text-transform:uppercase;
	color:#ffffff;
	font-weight:bolder;
	
	-webkit-transition: left 0.3s  linear;
	-webkit-transition-property:color;
	-moz-transition: left 0.3s linear;
	-moz-transition-property:color;
	-o-transition: left 0.3s  linear;
	-o-transition-property:color;
	-ms-transition: left 0.3s linear;
	-ms-transition-property:;
	transition: left 0.3s linear;
	transition-property:color;

		
}
#nav li a:hover, #nav li:hover a{color:#1d6688;}
#nav li.first{border-radius:30px 0px 0px 30px;-moz-border-radius:30px 0px 0px 30px;-webkit-border-radius:30px 0px 0px 30px;}
#nav li.last{border-radius:0px 30px 30px 0px;-moz-border-radius:0px 30px 30px 0px;-webkit-border-radius:0px 30px 30px 0px;}

/*------------------------------------ DROPDOWN -------------------------------------------------*/
/*------------------------------------ DROPDOWN -------------------------------------------------*/
/*------------------------------------ DROPDOWN -------------------------------------------------*/
.dropdown{
	
        margin:11px -2px;
        z-index: 1000;
        position:absolute!important;
        left:-990em!important;
        text-align:left!important;
        padding:1px 5px 0px 5px;
        list-style:none;
        width:auto;
      
}
#nav li:hover .dropdown{left:-2px!important;top:auto;}
#nav li.last .dropdown li{width:150px!important;padding:9px 0px 9px 5px!important;}

.dropdown li{
	
	/*float:none!important;*/
	min-width:150px;
	width:auto;
	max-width:300px;
	padding:10px 0px 10px 3px!important;
	border-top:1px solid #ffffff!important;
}
