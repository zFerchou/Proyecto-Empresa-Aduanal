
.listado {
    border: 1px solid #006680;
    background: #1D6688;
    border-radius: 5px;
    width: 72%;
    height: 30px;
    font-size: 13px;
    font-weight: bold;
    line-height: 30px;
    transition: all .3s;
}

.listado:hover {
    background: #FFFFFF;
    color: #fff;
}

.width25 {
    width: 25%;
}

.lbl {
    margin-left: 0px;
}

.errorAbajo
{
    background-color: red;
    padding: 6px 12px;
    border-radius: 4px;
    color: #fff;
    margin-left: -190px;
    margin-top: 33px;
	position: absolute;
	font-size: 13px;
    }
    .errorAbajo:before
    {
        content: '';
        border-top: 8px solid transparent;
        border-bottom: 8px solid red;
        border-right: 8px solid transparent;
        border-left: 8px solid transparent;
        left: 4px;
        position: absolute;
        top: -14px;
    }
