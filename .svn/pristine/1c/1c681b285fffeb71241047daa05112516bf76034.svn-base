.requerido{
    background-color: red;
    padding: 6px 12px;
    border-radius: 4px;
    color: #fff;
    margin-left: 50px;
    margin-top: 0px;
	position: absolute;
	font-size: 13px;
    }
    
    .requerido:before{
        content: '';
        border-top: 8px solid transparent;
        border-bottom: 8px solid red;
        border-right: 8px solid transparent;
        border-left: 8px solid transparent;
        left: 4px;
        position: absolute;
        top: -14px;
    }
    
