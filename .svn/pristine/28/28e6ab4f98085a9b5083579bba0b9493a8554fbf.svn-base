/*-------------------------------- TOOLTIP ------------------------------------------------------*/
/*-------------------------------- TOOLTIP ------------------------------------------------------*/
/*-------------------------------- TOOLTIP ------------------------------------------------------*/

.tooltip {display: inline;position: relative;}
  
.tooltip:hover:after {
    top: 40px;
    content: attr(data-tooltip);
    left: 20%;
    position: absolute;
    z-index:5000;
    background:#f4f4f4; /* el color de fondo */
    border-radius:5px;
    color: #135b7b; /* el color del texto */
    font-family: 'proxima_novalight';
    font-size:1.2em;
    padding: 5px 15px;
    text-align: center;
    width: 150px;
  }
.tooltip:hover:before {
    top: 34px;
    content: "";
    left: 30%;
    position: absolute;
    z-index:5000;
    /* el triángulo inferior */
    border: solid;
    border-color: #f4f4f4 transparent;
    border-width: 0 6px 6px 6px;
  }
 
